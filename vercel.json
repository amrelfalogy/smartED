{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/e-learning"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://130.162.171.106:8048/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
